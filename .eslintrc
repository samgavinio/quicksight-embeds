{
  "parser": "babel-eslint",
  "extends": "eslint:recommended",
  "rules": {
    "max-len": [1, 120, 2, { "ignoreComments": true }],
    "no-console": 0,
    "no-debugger": 0,
    "import/no-unresolved": "off",
    "import/no-extraneous-dependencies": "off",
    "import/extensions": "off",
    "no-param-reassign": [2, { "props": false }]
  },
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "globals": {
    "Templates": true,
    "$": true,
    "jQuery": true,
    "Handlebars": true,
    "_": true,
    "moment": true,
    "__dirname": true,
    "window": true,
    "process": true
  }
}
